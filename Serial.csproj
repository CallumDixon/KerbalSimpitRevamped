<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build"
         xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <AssemblyName>KerbalSimpitSerial</AssemblyName>
    <TargetFrameworkVersion>v4.6</TargetFrameworkVersion>
    <NoStdLib>true</NoStdLib>
    <NoSdkPath>true</NoSdkPath>
    <OutputType>Library</OutputType>
    <OutputPath>SerialBin\</OutputPath>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="mscorlib">
      <HintPath>$(KSPLIBDIR)/mscorlib.dll</HintPath>
    </Reference>
    <Reference Include="System.Core">
      <HintPath>$(KSPLIBDIR)/System.Core.dll</HintPath>
    </Reference>
    <Reference Include="System">
      <HintPath>$(KSPLIBDIR)/System.dll</HintPath>
    </Reference>
    <Reference Include="Assembly-CSharp">
      <HintPath>$(KSPLIBDIR)/Assembly-CSharp.dll</HintPath>
      <Private>False</Private>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <Compile Include="KerbalSimpit.IO.Ports\*.cs" />
    <Compile Include="Properties\SerialAssemblyInfo.cs" />
  </ItemGroup>
  <Import Project="$(MSBuildToolsPath)\Microsoft.CSharp.targets" />
</Project>
